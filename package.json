{
	"name": "apex-remote-terminal",
	"displayName": "Salesforce Apex Remote Terminal",
	"description": "Remote Apex code terminal",
	"version": "1.5.0",
	"icon": "images/icon.png",
	"publisher": "UmbertoPiscopo",
	"repository": "https://github.com/Umb40k/up.apex-script-terminal",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Extension Packs",
		"Other"
	],
	"keywords": [
		"sfdx",
		"salesforcedx",
		"sfdx apex code",
		"salesforce",
		"salesforce apex code editor",
		"salesforce console apex script",
		"salesforce script"
	],
	"activationEvents": [
		"onCommand:apex-remote.terminal"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "apex-remote.terminal",
				"title": "SFDX Start Apex Terminal: Enter Apex Code",
				"when": "sfdx:project_opened"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -w -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/jquery": "^3.5.6",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"ace-builds": "^1.4.12"
	}
}
